{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": [
      "https://white-hill-0cb38030f.2.azurestaticapps.net",
      "http://localhost:3000",
      "http://localhost:3001",
      "*",
      "http://localhost:8080"
    ],
    "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD"],
    "allowedHeaders": [
      "Content-Type", 
      "Authorization", 
      "X-Requested-With",
      "Accept",
      "Origin",
      "Access-Control-Allow-Origin"
    ],
    "supportCredentials": false
  }
}
